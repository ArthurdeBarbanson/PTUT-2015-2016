  </route>
  <route id="responsable_ajout_responsable" path="/responsable/ajout/responsable">
    <default key="_controller">SiteBundle:Responsable:ajouterNouveauResponsableLicence</default>
  </route>
  <route id="site_detailsAnnonceResponsable" path="/responsable/details_annonce/{offreId}">
    <default key="_controller">SiteBundle:Responsable:detailAnnonce</default>
  </route>
  <route id="site_AssigneTuteurResponsable" path="/responsable/assigner_tuteur/{etudiantId}">
    <default key="_controller">SiteBundle:Responsable:ajouterTriplette</default>
  </route>
  <route id="validation_Annonce" path="/responsable/validation_annonce/{offreId}">
    <default key="_controller">SiteBundle:Responsable:validerAnnonce</default>
  </route>
  <route id="validation_etat_etudiant" path="/responsable/validation-etape">
