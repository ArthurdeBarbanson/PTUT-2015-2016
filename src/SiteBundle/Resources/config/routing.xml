  <route id="site_impressionPostulant" path="/entreprise/impressionPostulants/{postulantId}">
    <default key="_controller">SiteBundle:Entreprise:impressionPostulants</default>
  </route>
  <route id="site_supprAnnonce" path="/entreprise/supprimeAnnonnce/{annonceId}">
    <default key="_controller">SiteBundle:Entreprise:supprAnnonce</default>
  </route>
  <!-- Etudiant -->
  <route id="detailsEntreprise" path="/etudiant/details_entreprise/{entrepriseId}">
    <default key="_controller">SiteBundle:Etudiant:detailsEntreprise</default>
  </route>
  <route id="modif_mot_de_passe" path="/etudiant/modification_mot_de_passe">
    <default key="_controller">SiteBundle:Etudiant:modificationMotPasse</default>
  </route>
  <route id="site_detailsAnnonceEtudiant" path="/etudiant/details_annonce/{annonceId}">
    <default key="_controller">SiteBundle:Default:detailAnnonce</default>
  </route>
  <route id="site_accepterOffreEtudiant" path="/etudiant/accepter_offre/{annonceId}">
