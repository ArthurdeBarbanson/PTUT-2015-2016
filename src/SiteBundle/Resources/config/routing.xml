  </route>
  <route id="gestion_email" path="/responsable/email/etape">
    <default key="_controller">SiteBundle:Responsable:email</default>
  </route>
  <!--security-->
  <route id="login" path="/login">
    <default key="_controller">SiteBundle:Security:login</default>
  </route>
  <route id="login_check" path="/login_check"/>
  <route id="logout" path="/logout"/>
  <route id="test" path="/etudiant/test">
    <default key="_controller">SiteBundle:Default:test</default>
  </route>
  <route id="ajout" path="/ajoutReynald">
    <default key="_controller">SiteBundle:Security:createUser</default>
  </route>
  <!--test-->
  <route id="testAdresseForm" path="/testAdresseForm">
    <default key="_controller">SiteBundle:Test:adresseFormTest</default>
  </route>
