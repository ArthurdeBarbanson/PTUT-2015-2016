<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Acceuil!{% endblock %}</title>

    {% block stylesheets %}
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="{{ absolute_url(asset('css/style/bootstrap.css')) }}"/>

    <!-- Optional theme -->
    <link rel="stylesheet" href="{{ absolute_url(asset('css/style/bootstrap-theme.css')) }}"/>
    <link rel="stylesheet" href="{{ absolute_url(asset('css/style/styleImpression.css')) }}"/>
    <link rel="stylesheet" href="{{ absolute_url(asset('css/style/font-awesome.css')) }}"/>
    {% endblock %}
    <!-- Latest compiled and minified JavaScript -->
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
<div id="wrapper">
    <div id="content">
        <div class="container mainContainer">
            {% block body %}{% endblock %}
        </div>
    </div>
</div>
{% block javascripts %}{% endblock %}
<script src="https://code.jquery.com/jquery-2.2.0.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>

</body>
</html>
